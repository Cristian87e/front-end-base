{
  "name": "front-end-base",
  "version": "1.0.0",
  "description": "Webpack 2, React, Redux, CSS-Next",
  "main": "index.js",
  "scripts": {
    "dev": "npm run eslint && webpack-dev-server --config ./config/webpack.config.dev.js",
    "prod": "npm run eslint && npm run coverage && npm run clean && npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --config  ./config/webpack.config.prod.js",
    "clean": "rimraf ./dist/*",
    "test": "jest",
    "coverage": "jest --coverage",
    "eslint": "eslint src test"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cristian87e/front-end-base.git"
  },
  "author": "Cristian Perrone",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cristian87e/front-end-base/issues"
  },
  "homepage": "https://github.com/Cristian87e/front-end-base#readme",
  "dependencies": {
    "css-loader": "^0.27.3",
    "enzyme": "^2.8.0",
    "html-webpack-plugin": "^2.28.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.2.4",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "postcss": "^5.2.16",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.1",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
